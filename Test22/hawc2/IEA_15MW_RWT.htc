; IEA 15 MW Reference Wind Turbine. Model update from commit "7d179ee".
; 
begin simulation ;
  time_stop    100.0 ;
  solvertype   1 ;    (newmark)
  on_no_convergence continue ;
  convergence_limits 1E3 1.0 1E-7 ;
  logfile ./log/IEA_15MW_RWT.log ;
  begin newmark;
    deltat    0.01;  
  end newmark;
end simulation;
;
